
public class Building {

	//VARIABLES
	String name, address;
	
	//CONSTRUCTORS
	public Building(){
		name = null;
		address = null;
	}
	public Building(String name, String address){
		this.name = name;
		this.address = address;
	}
	
	//SET FUNCTIONS
	public void setName(String name){
		this.name = name;
	}
	public void setAddress(String address){
		this.address = address;
	}
	
	//GET FUNCTIONS
	public String getName(){
		return name;
	}
	public String getAddress(){
		return address;
	}


}
